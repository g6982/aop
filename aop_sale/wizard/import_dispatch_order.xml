<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>

        <record id="view_import_dispatch_order_wizard_form" model="ir.ui.view">
            <field name="name">import.dispatch.order.wizard</field>
            <field name="model">import.dispatch.order.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="file"/>
                    </group>

                    <footer>
                        <button name="start_import_purchase_order" string="Import Dispatch Orders" class="oe_highlight"
                                type="object"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_import_dispatch_order" model="ir.actions.act_window">
            <field name="name">Import Dispatch Order</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">import.dispatch.order.wizard</field>
            <field name="target">new</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
        </record>

        <!--
        <menuitem id="import_dispatch_order"
                  name="Import Dispatch Order"
                  action="action_import_dispatch_order"
                  parent="stock.menu_stock_warehouse_mgmt"/>
         -->

    </data>
</odoo>